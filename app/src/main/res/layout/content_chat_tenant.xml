<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".ChatTenant"
    tools:showIn="@layout/activity_chat_tenant">

    <TextView
        android:id="@+id/tenant_chat_maintenance_text_content"
        android:layout_width="253dp"
        android:layout_height="53dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="You: Boa tarde, Pedro. Ser치 que me podia verificar as leituras ..."
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/tenant_chat_maintenance_pic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tenant_chat_maintenance_pic"
        app:layout_constraintTop_toBottomOf="@+id/tenant_chat_maintenance_name" />

    <TextView
        android:id="@+id/tenant_chat_manager_text_content"
        android:layout_width="253dp"
        android:layout_height="53dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="You: Bom dia Sr Joaquim! Era s칩 para informar que j치  ..."
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/tenant_chat_manager_pic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tenant_chat_manager_pic"
        app:layout_constraintTop_toBottomOf="@+id/tenant_chat_manager_name" />

    <TextView
        android:id="@+id/tenant_chat_maintenance_name"
        android:layout_width="253dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="Pedro Marques"
        android:textColor="@android:color/secondary_text_light"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tenant_chat_maintenance_pic"
        app:layout_constraintTop_toTopOf="@+id/tenant_chat_maintenance_pic" />

    <TextView
        android:id="@+id/tenant_chat_manager_name"
        android:layout_width="253dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:text="Joaquim In치cio"
        android:textColor="@android:color/secondary_text_light"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tenant_chat_manager_pic"
        app:layout_constraintTop_toTopOf="@+id/tenant_chat_manager_pic" />

    <ImageView
        android:id="@+id/go_back_chat_tenant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="0.02"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/back_icon" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="8dp"
        android:text="Chat"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="157dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="269dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="381dp" />

    <ImageView
        android:id="@+id/tenant_chat_manager_pic"
        android:layout_width="133dp"
        android:layout_height="133dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        app:srcCompat="@mipmap/manager_profile_pic" />

    <ImageView
        android:id="@+id/tenant_chat_maintenance_pic"
        android:layout_width="133dp"
        android:layout_height="133dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tenant_chat_manager_pic"
        app:layout_constraintVertical_bias="0.006"
        app:srcCompat="@mipmap/maintenance_profile_pic" />

    <ImageView
        android:id="@+id/manager_private_chat"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/maintenance_private_chat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tenant_chat_manager_pic"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/maintenance_private_chat"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/tenant_chat_maintenance_pic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tenant_chat_maintenance_pic"
        app:layout_constraintTop_toTopOf="@+id/tenant_chat_maintenance_pic"
        app:srcCompat="@android:color/transparent" />
</android.support.constraint.ConstraintLayout>